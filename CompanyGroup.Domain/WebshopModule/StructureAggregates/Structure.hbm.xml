<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <sql-query name="InternetUser.StructureSelect" callable="true">
    <query-param name="DataAreaId" type="System.String"/>
    <query-param name="Discount" type="System.Boolean"/>    
    <query-param name="SecondHand" type="System.Boolean"/>
    <query-param name="New" type="System.Boolean"/>
    <query-param name="Stock" type="System.Boolean"/>
    <query-param name="FindText" type="System.String"/>
    <query-param name="PriceFilter" type="System.String"/>
    <query-param name="PriceFilterRelation" type="System.Int32"/>
    <return-scalar column="Id" type="System.String"/>
    <return-scalar column="Title" type="System.String"/>
    <return-scalar column="Description" type="System.String"/>
    <return-scalar column="HtmlPath" type="System.String"/>
    <return-scalar column="EndDate" type="System.DateTime"/>
    <return-scalar column="PicturePath" type="System.String"/>
    <return-scalar column="AllowedDate" type="System.DateTime"/>
    <return-scalar column="AllowedTime" type="System.Int32"/>
    exec InternetUser.StructureSelect @DataAreaId=:DataAreaId, @Discount=:Discount, @SecondHand=:SecondHand, @New=:New, @Stock=:Stock, @FindText=:FindText, @PriceFilter=:PriceFilter, @PriceFilterRelation=:PriceFilterRelation
  </sql-query>
</hibernate-mapping>