<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <sql-query name="InternetUser.cms_AddressZipCode" callable="true">
    <query-param name="Prefix" type="System.String"/>
    <query-param name="DataAreaId" type="System.String"/>
    <return-scalar column="ZipCode" type="System.String"/>
    exec InternetUser.cms_AddressZipCode @Prefix=:Prefix, @DataAreaId=:DataAreaId
  </sql-query>
</hibernate-mapping>  