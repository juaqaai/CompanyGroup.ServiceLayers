<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Visitor" lazy="false">
    <id name="Id">
      <generator class="identity" />
    </id>
    <property name="VisitorId" />
    <property name="LoginIP" />
    <property name="RecId" />
    <property name="CustomerId" />
    <property name="CustomerName" />
    <property name="PersonId" />
    <property name="PersonName" />
    <property name="Email" />
    <property name="IsWebAdministrator" />
    <property name="InvoiceInfoEnabled" />
    <property name="PriceListDownloadEnabled" />
    <property name="CanOrder" />
    <property name="RecieveGoods" />
    <property name="PaymTermId" />
    <property name="Currency" />
    <property name="LanguageId" />
    <property name="PriceGroupId" />
    <property name="InventLocation" />    
    <!--<property name="LoggedIn" />-->
    <property name="LoginType" />
    <property name="PartnerModel" />
    <property name="AutoLogin" />
    <property name="LoginDate" />
    <property name="LogoutDate" />
    <property name="ExpiredDate" />
    <property name="DataAreaId" />
    <property name="Valid" />
    <property name="RepresentativeId" />
    <property name="RepresentativeName" />
    <property name="RepresentativePhone" />
    <property name="RepresentativeMobile" />
    <property name="RepresentativeExtension" />
    <property name="RepresentativeEmail" />    
  </class>
  <sql-query name="InternetUser.SignIn" callable="true">
    <query-param name="UserName" type="System.String"/>
    <query-param name="Password" type="System.String"/>
    <query-param name="DataAreaId" type="System.String"/>
    <return class="Visitor"></return>
    exec InternetUser.SignIn @UserName=:UserName,  @Password=:Password, @DataAreaId=:DataAreaId
  </sql-query>
</hibernate-mapping>

