          @person.ContactPersonId <br/>
          @person.Email<br/>
          
          @person.EmailArriveOfGoods<br/>
          @person.EmailOfDelivery<br/>
          @person.EmailOfOrderConfirm<br/>
          @person.FirstName<br/>
          @person.InvoiceInfo<br/>
          @person.LastName<br/>
          
          @person.LeftCompany<br/>
          @person.Newsletter<br/>
          @person.Password<br/>
          @person.PriceListDownload<br/>
          @person.SmsArriveOfGoods<br/>
          @person.SmsOfDelivery<br/>
          @person.SmsOrderConfirm<br/>
          @person.Telephone<br/>
          @person.UserName<br/>
          @person.WebAdmin<br/>}</p></td>
  </tr>
